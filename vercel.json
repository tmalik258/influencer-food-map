{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/restaurants",
      "destination": "/api/restaurants.py"
    },
    {
      "source": "/api/restaurants/(.*)",
      "destination": "/api/restaurants.py?path=$1"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/influencers",
      "destination": "/api/influencers.py"
    },
    {
      "source": "/api/listings",
      "destination": "/api/listings.py"
    },
    {
      "source": "/api/tags",
      "destination": "/api/tags.py"
    },
    {
      "source": "/api/videos",
      "destination": "/api/videos.py"
    }
  ]
}
