YOUTUBE_API_KEY=Credential Key from Google Console
SUPABASE_DB_URL=postgresql://username:password@db.supabase.co:5432/postgres
GOOGLE_PLACES_API_KEY=Your Google Places API Key from Google Console

# Browserless API Configuration (for production browser automation)
# This enables secure browser automation in production environments
BROWSERLESS_WS_URL=ws://browserless:3000
BROWSERLESS_TOKEN=your_browserless_token_here
BROWSERLESS_DEBUG=false

# Optional: yt-dlp cookie/auth configuration to bypass YouTube bot checks
# Provide either a cookies file exported from your browser OR use cookies from browser
# If both are set, cookies file takes precedence
YTDLP_COOKIES_FILE=/path/to/cookies.txt
# or use cookies from browser (supported: chrome, edge, firefox)
YTDLP_COOKIES_FROM_BROWSER=chrome
# optional: specify profile name/id when using cookies from browser
YTDLP_BROWSER_PROFILE=Default
# Optional geo-bypass (helps with some regional issues)
YTDLP_GEO_BYPASS=true
YTDLP_GEO_COUNTRY= # Leave empty for auto-detection

# Google Login Credentials (required for YouTube cookie refresh)
GOOGLE_EMAIL=your_google_email@gmail.com
GOOGLE_PASSWORD=your_google_password

# Admin Credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=your_admin_password

# Redis Configuration
REDIS_URL=redis://localhost:6379

# OpenAI API Key (for content generation)
OPENAI_API_KEY=your_openai_api_key

# Pexels API Key (for restaurant images)
PEXELS_API_KEY=your_pexels_api_key

# Influencer YouTube Channels (comma-separated)
INFLUENCER_CHANNELS=UCBa659QWEk1AI4Tg--mrJ2A,UCJFp8uSYCjXOMnkUyb3CQ3Q